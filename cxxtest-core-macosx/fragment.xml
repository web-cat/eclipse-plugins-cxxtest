<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<fragment>
   <extension
         point="net.sf.webcat.eclipse.cxxtest.extraProjectOptions">
      <extraProjectOptions
            id="net.sf.webcat.eclipse.cxxtest.macosx.stackTracingOptions">
         <optionSet
               version="1.5.0">
            <runnerIncludes
                  path="/runner-include">
               <includePath
                     path="dereferee-src/gcc_macosx_platform.cpp">
               </includePath></runnerIncludes>
            <enablement
                  configurationFilter="Debug"
                  isForStackTrace="true">
               <tool
                     superClassId="cdt.managedbuild.tool.gnu.cpp.compiler">
                  <splitStringOption
                        id="gnu.cpp.compiler.option.other.other">
                     <item
                           value="-finstrument-functions">
                     </item>
                     <item
                           value="-gstabs">
                     </item>
                  </splitStringOption>
               </tool>
            </enablement>
         </optionSet>
      </extraProjectOptions>
   </extension>

</fragment>
